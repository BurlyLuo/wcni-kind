/**** QToolBox (enabled) ****/
QToolBox
{
  background-color: transparent;
}

QToolBox::tab
{
  background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #D1E1E1, stop: 0.4 #BDBDBD, stop: 0.5 #B8B8B8, stop: 1.0 #A3A3A3);      
  border: 1px solid rgb(20,20,20);
  color: rgb(20,20,20);
  }

QToolBox::tab:hover,
QToolBox::tab:selected:hover
{
  background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #C1D1E1, stop: 0.4 #ADADBD, stop: 0.5 #A8A8B8, stop: 1.0 #9393A3);      
}

QToolBox::tab:selected
{
  background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 rgb(180,210,240), stop: 0.4 rgb(150,180,230), stop: 0.5 rgb(140,170,220), stop: 1.0 rgb(120,150,200));      
  color: rgb(20,20,20);
}

QToolBox QScrollArea
{
  background-color: rgb(100,100,100);
  border: 0px solid transparent;
}


QToolBox QScrollArea:disabled
{
  background-color: rgb(120,120,120);
  border-color: transparent;
}


