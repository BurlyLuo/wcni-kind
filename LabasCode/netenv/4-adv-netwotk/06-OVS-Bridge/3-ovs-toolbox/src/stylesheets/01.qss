QScrollBar:vertical {
     border: 2px solid grey;
     background: lightgrey;
     min-height: 20px;
 }
 
 QScrollBar::handle:vertical {
     background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #D1E1E1, stop: 0.4 #BDBDBD, stop: 0.5 #B8B8B8, stop: 1.0 #A3A3A3);
     border: 2px solid grey;
     border-style: outset;
     min-height: 10px;
 }

/* fleche bas */
 QScrollBar::add-line:vertical {
     border: 2px solid grey;
     image: url(':/images/chevron-down.png');
     background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #D1E1E1, stop: 0.4 #BDBDBD, stop: 0.5 #B8B8B8, stop: 1.0 #A3A3A3);
     border-style: outset;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
}

/*fleche haut */
 QScrollBar::sub-line:vertical {
     border: 2px solid grey;
     image: url(':/images/chevron-up.png');
     background: qlineargradient(x1: 1, y1: 0, x2: 0, y2: 0, stop: 0 #D1E1E1, stop: 0.4 #BDBDBD, stop: 0.5 #B8B8B8, stop: 1.0 #A3A3A3);
     subcontrol-position: top;
     subcontrol-origin: margin;
}


QScrollBar:horizontal {
     border: 2px solid grey;
     background: lightgrey;
     min-width: 20px;
 }

QScrollBar::handle:horizontal {
     background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #D1E1E1, stop: 0.4 #BDBDBD, stop: 0.5 #B8B8B8, stop: 1.0 #A3A3A3);
     border: 2px solid grey;
     border-style: outset;
     min-width: 10px;
 }

/* fleche bas */
 QScrollBar::add-line:horizontal {
     border: 2px solid grey;
     image: url(':/images/chevron-right.png');
     background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #D1E1E1, stop: 0.4 #BDBDBD, stop: 0.5 #B8B8B8, stop: 1.0 #A3A3A3);
     border-style: outset;                
     subcontrol-position: right;
     subcontrol-origin: margin;
}

/*fleche gauche */
 QScrollBar::sub-line:horizontal {
     border: 2px solid grey;
     image: url(':/images/chevron-left.png');
     background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #D1E1E1, stop: 0.4 #BDBDBD, stop: 0.5 #B8B8B8, stop: 1.0 #A3A3A3);
     subcontrol-position: left;
     subcontrol-origin: margin;
}




QTableWidget::item {
   padding: 5px 0px 5px 0px;        
}

QTableView {
   border: 1px solid black; /*border qtablewidget */    
}



QTabWidget::pane { /* The tab widget frame */
    border: 3px solid black;
}

QTabWidget::tab-bar {
    left: 5px; /* move to the right by 5px */
    background: red;    	    
}


QTabBar::tab {
    background: rgb(180,180,200,255);
    border: 1px solid rgb(100,100,220,255);
    border-bottom-color: #C2C7CB; /* same as the pane color */
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 1ex;
    padding: 2px;
}


QTabBar::tab[subtab=true] {
    background: red;
    border: 2px solid #C4C4C3;
    border-bottom-color: #C2C7CB; /* same as the pane color */
    border-top-left-radius: 16px;
    border-top-right-radius: 16px;
    min-width: 8ex;
    padding: 10px;
}

QTabBar::tab:selected {
    background: rgb(150,150,230,255);
}

QTabBar::tab:hover {
    background: rgb(180,250,180,255);
}


