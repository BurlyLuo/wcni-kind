/* item selectionne  dans liste deroulee */
QComboBox:!editable:on {
background: rgb(50,120,50,255);
color: rgb(100,200,100,255);
border: 2px;
}

/* bouton fleche bas */
QComboBox[enabled=true] {
background: rgb(220,220,255,255);
color: rgb(10,10,10,255);
border: 2px;
}

/* texte quand liste ferme*/
QComboBox:!editable {
border: 2px solid #6666cc;
border-radius: 6px;
border-style: outset;
selection-background-color: rgb(210,240,210);
selection-color: rgb(0,0,0);
color: rgb(0,0,0,255);
/* background-color: rgb(230,230,255,255); */
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #D1E1E1, stop: 0.4 #BDBDBD, stop: 0.5 #B8B8B8, stop: 1.0 #A3A3A3);

padding-left: 5px;
padding-right: 5px;
min-width: 20px;
min-height: 20px;

}


QComboBox:!editable:disabled {
border: 2px solid #6666cc;
border-radius: 6px;
border-style: outset;
color: rgb(160,160,160,255);
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #E1E1F1, stop: 0.4 #B9B9FF, stop: 0.5 #B9B9FF, stop: 1.0 #C3C3D3);

min-width: 30px;
min-height: 20px;
}





